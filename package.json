{
  "name": "transcendence",
  "version": "1.0.0",
  "description": "A Node.js + Typescript + Fastify + Tailwind CSS server",
  "scripts": {
    "dev": "ts-node src/main.ts",
    "build:css": "postcss client/src/css/input.css -o client/dist/output.css --env production",
    "watch:css": "postcss client/src/css/input.css -o client/dist/output.css --watch",
    "build:client": "tsc -p tsconfig.client.json",
    "bundle": "esbuild client/src/app.ts --bundle --outfile=client/dist/app.js --platform=browser --format=esm --sourcemap",
    "watch:client": "tsc -p tsconfig.client.json --watch",
    "build": "tsc && npm run build:css",
    "start": "node dist/main.js"
  },
  "dependencies": {

    "@babylonjs/materials": "^6.0.0",
    "@babylonjs/core": "^6.0.0",
    "@babylonjs/gui": "^6.0.0",
    "@babylonjs/loaders": "^6.0.0",
    "@fastify/cookie": "^7.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/static": "^6.12.0",
    "autoprefixer": "^10.4.13",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.19.0",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.0.0",
    "fastify-static": "^4.6.1",
    "jsonwebtoken": "^9.0.2",
    "node-vault": "^0.10.0",
    "postcss-import": "^15.1.0",
    "sharp": "^0.33.5",
    "simpleheat": "^0.4.0",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.0.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.0.0",
    "@babylonjs/gui": "^6.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.19",
    "@types/request": "^2.48.12",
    "@types/speakeasy": "^2.0.10",
    "@types/sqlite3": "^3.1.10",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.19.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
